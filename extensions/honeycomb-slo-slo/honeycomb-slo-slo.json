{
    "typeName": "Honeycomb::SLO::SLO",
    "description": "Honeycomb SLOs allows you to define and monitor Service Level Objectives (SLOs) for your organization.",
    "sourceUrl": "https://github.com/honeycombio/cloudformation-integrations.git",
    "typeConfiguration": {
        "properties": {
          "HoneycombConfig": {
            "$ref": "#/definitions/HoneycombConfig"
          }
        },
        "additionalProperties": false
      },
    "definitions": {
        "HoneycombConfig": {
            "properties": {
              "ApiKey": {
                "description": "Honeycomb API key",
                "type": "string"
              },
              "ApiUrl": {
                "description": "Honeycomb API URL (defaults to https://api.honeycomb.io)",
                "type": "string",
                "default": "https://api.honeycomb.io"
              }
            },
            "required": [
              "ApiKey"
            ],
            "type": "object",
            "additionalProperties": false
        }
    },
    "properties": {
        "ID": {
            "description": "The ID of the SLO.",
            "type": "string"
        },
        "Name": {
            "description": "The name of the SLO.",
            "type": "string",
            "minLength": 1,
            "maxLength": 120
        },
        "Dataset": {
            "description": "The dataset this SLO is created in. Must be the same dataset as the SLI.",
            "type": "string"
        },
        "Description": {
            "description": "A description of the SLO.",
            "type": "string",
            "minLength": 0,
            "maxLength": 1023
        },
        "TimePeriod": {
            "description": "The time period, in days, over which your SLO will be evaluated.",
            "type": "integer",
            "minimum": 1
        },
        "TargetPercentage": {
            "description": "The percentage of qualified events that you expect to succeed during the time period.",
            "type": "number",
            "minimum": 1.00000,
            "maximum": 99.9999
        },
        "SLI": {
            "description": "The alias of the Derived Column that will be used as the SLI to indicate event success.",
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Dataset",
        "Name",
        "TargetPercentage",
        "TimePeriod",
        "SLI"
    ],
    "readOnlyProperties": [
        "/properties/ID"
    ],
    "primaryIdentifier": [
        "/properties/ID"
    ],
    "handlers": {
        "create": {
            "permissions": []
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": []
        },
        "delete": {
            "permissions": []
        },
        "list": {
            "permissions": []
        }
    }
}
